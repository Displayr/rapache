<% 
class<-'e' 
hrefify <- function(title){
	gsub("[\\.()]",'_',title,perl=TRUE)
}
zebra <- function(title,l){
	cat('<h2><a name="',hrefify(title),'"> </a>',title,'</h2><table><tbody>',sep='')
	if (is.list(l)){
		for (i in names(l)) { 
			class <- ifelse(class=='e','o','e')
			%><tr class="<%=class%>"><td class="l"><%=i%></td><td><%=as.character(l[[i]])%></td></tr><%
		}
	} else {
		for (i in l) { 
			class <- ifelse(class=='e','o','e')
			%><tr class="<%=class%>"><td><%=as.character(i)%></tr><%
		}
	}
	cat('</tbody></table>')
}
-%>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "DTD/xhtml1-transitional.dtd">
<html><head>
<style type="text/css">
body { font-family: "lucida grande",verdana,sans-serif; margin-left: 210px; }
